{
  "name": "xutl/laravel-payment",
  "description": "This is a Laravel expansion for the Payment.",
  "keywords": [
    "laravel",
    "payment"
  ],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.0",
    "ext-openssl": "*",
    "ext-simplexml": "*",
    "ext-json": "*",
    "ext-dom": "*",
    "ext-libxml": "*",
    "xutl/supports": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "XuTL\\Payment\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "XuTL\\Payment\\PaymentServiceProvider"
      ],
      "aliases": {
        "Payment": "XuTL\\Payment\\Payment"
      }
    }
  },
  "prefer-stable": true
}
